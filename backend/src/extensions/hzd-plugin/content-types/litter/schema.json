{
  "kind": "collectionType",
  "collectionName": "litters",
  "info": {
    "singularName": "litter",
    "pluralName": "litters",
    "displayName": "Litter"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "content-manager": {
      "visible": true
    }
  },
  "attributes": {
    "breeder": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::hzd-plugin.breeder",
      "inversedBy": "litters"
    },
    "mother": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::hzd-plugin.dog"
    },
    "stuntDog": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::hzd-plugin.dog"
    },
    "dateOfManting": {
      "type": "date"
    },
    "expectedDateOfBirth": {
      "type": "date"
    },
    "dateOfBirth": {
      "type": "date"
    },
    "LitterStatus": {
      "type": "enumeration",
      "required": true,
      "default": "Planned",
      "enum": [
        "Planned",
        "Manted",
        "Littered",
        "Closed"
      ]
    },
    "AmountRS": {
      "type": "component",
      "component": "breeding.puppy-amount",
      "repeatable": false
    },
    "AmountRSM": {
      "type": "component",
      "component": "breeding.puppy-amount",
      "repeatable": false
    },
    "AmountRB": {
      "type": "component",
      "component": "breeding.puppy-amount",
      "repeatable": false
    },
    "AmountHS": {
      "type": "component",
      "component": "breeding.puppy-amount",
      "repeatable": false
    },
    "AmountHSM": {
      "type": "component",
      "component": "breeding.puppy-amount",
      "repeatable": false
    },
    "AmountHB": {
      "type": "component",
      "component": "breeding.puppy-amount",
      "repeatable": false
    },
    "StatusMessage": {
      "type": "text"
    },
    "StatusMessageDraft": {
      "type": "text"
    },
    "StatusMessageDirtyFlag": {
      "type": "boolean"
    },
    "OrderLetter": {
      "type": "string",
      "maxLength": 1,
      "required": true,
      "regex": "[A-Z]"
    },
    "PuppyImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
  }
}
