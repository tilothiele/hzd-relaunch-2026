{
  "kind": "collectionType",
  "collectionName": "calendar_entries",
  "info": {
    "singularName": "calendar-entry",
    "pluralName": "calendar-entries",
    "displayName": "CalendarEntry"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Headline": {
      "type": "string",
      "required": true
    },
    "Description": {
      "type": "text"
    },
    "Date": {
      "type": "date",
      "required": true
    },
    "Time": {
      "type": "time"
    },
    "CalendarDocument": {
      "type": "component",
      "component": "calendar.calendar-document",
      "repeatable": true
    },
    "calendar": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::calendar.calendar"
    },
    "Kind": {
      "type": "enumeration",
      "enum": [
        "HZD",
        "Beurteilungen",
        "Zuchttag",
        "CAC",
        "CACIB"
      ]
    },
    "AnmeldeLink": {
      "type": "string"
    },
    "ErgebnisLink": {
      "type": "string"
    },
    "LongDescription": {
      "type": "blocks"
    }
  }
}
