{
  "kind": "collectionType",
  "collectionName": "breeders",
  "info": {
    "singularName": "breeder",
    "pluralName": "breeders",
    "displayName": "Breeder"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "content-manager": {
      "visible": true
    }
  },
  "attributes": {
    "member": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::hzd-plugin.member"
    },
    "kennelName": {
      "type": "string",
      "maxLength": 200
    },
    "breedingLicenseSince": {
      "type": "date"
    },
    "litters": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::hzd-plugin.litter",
      "mappedBy": "breeder"
    }
  }
}