{
  "kind": "collectionType",
  "collectionName": "breeders",
  "info": {
    "singularName": "breeder",
    "pluralName": "breeders",
    "displayName": "Breeder"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "content-manager": {
      "visible": true
    }
  },
  "attributes": {
    "kennelName": {
      "type": "string",
      "maxLength": 200
    },
    "breedingLicenseSince": {
      "type": "date"
    },
    "Disable": {
      "type": "boolean",
      "default": false
    },
    "IsActive": {
      "type": "boolean"
    },
    "litters": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::hzd-plugin.litter",
      "mappedBy": "breeder"
    },
    "BreedersIntroduction": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    },
    "cId": {
      "type": "integer",
      "unique": true
    },
    "member": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "avatar": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "WebsiteUrl": {
      "type": "string"
    },
    "WebsiteUrlDraft": {
      "type": "string"
    },
    "Address": {
      "type": "component",
      "component": "personal.address",
      "repeatable": false
    },
    "InternalNotes": {
      "type": "text"
    },
    "BreedersIntroDraft": {
      "type": "text"
    },
    "isDirty": {
      "type": "boolean"
    },
    "owner_member": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "BreederEmail": {
      "type": "email"
    }
  }
}
