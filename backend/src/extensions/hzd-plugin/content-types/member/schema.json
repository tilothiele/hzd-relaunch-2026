{
  "kind": "collectionType",
  "collectionName": "members",
  "info": {
    "singularName": "member",
    "pluralName": "members",
    "displayName": "Member"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "content-manager": {
      "visible": true
    }
  },
  "attributes": {
    "memberSince": {
      "type": "date"
    },
    "membershipNo": {
      "type": "integer"
    },
    "cancellationOn": {
      "type": "date"
    },
    "cancellationDueDate": {
      "type": "date"
    },
    "adress1": {
      "type": "string",
      "maxLength": 100
    },
    "adress2": {
      "type": "string",
      "maxLength": 100
    },
    "zip": {
      "type": "string",
      "maxLength": 10
    },
    "countryCode": {
      "type": "string",
      "maxLength": 2
    },
    "phone": {
      "type": "string",
      "maxLength": 50
    },
    "sex": {
      "type": "enumeration",
      "enum": [
        "M",
        "F"
      ]
    },
    "dateOfBirth": {
      "type": "date"
    },
    "dateOfDeath": {
      "type": "date"
    },
    "user": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user",
      "mappedBy": "member"
    },
    "fullName": {
      "type": "string"
    },
    "region": {
      "type": "enumeration",
      "enum": [
        "Nord",
        "Ost",
        "Mitte",
        "SÃ¼d",
        "West"
      ]
    },
    "Avatar": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "Intro": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    },
    "officer_roles": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::officer-role.officer-role",
      "inversedBy": "members"
    }
  }
}
