{
  "kind": "collectionType",
  "collectionName": "dogs",
  "info": {
    "singularName": "dog",
    "pluralName": "dogs",
    "displayName": "Dog"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "content-manager": {
      "visible": true
    }
  },
  "attributes": {
    "givenName": {
      "type": "string",
      "maxLength": 100
    },
    "fullKennelName": {
      "type": "string",
      "maxLength": 500
    },
    "avatar": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "microchipNo": {
      "type": "string",
      "maxLength": 30
    },
    "stuntLicenseSince": {
      "type": "date"
    },
    "dateOfBirth": {
      "type": "date"
    },
    "dateOfDeath": {
      "type": "date"
    },
    "sex": {
      "type": "enumeration",
      "enum": [
        "M",
        "F"
      ]
    },
    "color": {
      "type": "enumeration",
      "enum": [
        "S",
        "SM",
        "B"
      ]
    },
    "father": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::hzd-plugin.dog",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "sex"
            },
            "F"
          ]
        }
      }
    },
    "mother": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::hzd-plugin.dog",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "sex"
            },
            "F"
          ]
        }
      }
    },
    "cId": {
      "type": "integer",
      "unique": true
    },
    "cBreederId": {
      "type": "integer"
    },
    "cOwnerId": {
      "type": "integer"
    },
    "SOD1": {
      "type": "enumeration",
      "enum": [
        "N_N",
        "N_DM",
        "DM_DM"
      ]
    },
    "HD": {
      "type": "enumeration",
      "enum": [
        "A1",
        "A2",
        "B1",
        "B2"
      ]
    },
    "Genprofil": {
      "type": "boolean"
    },
    "EyesCheck": {
      "type": "boolean"
    },
    "HeartCheck": {
      "type": "boolean"
    },
    "ColorCheck": {
      "type": "boolean"
    },
    "MemosDraft": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    },
    "MemosReleased": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    },
    "Location": {
      "type": "component",
      "component": "breeding.geo-location",
      "repeatable": false
    },
    "owner": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "breeder": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::hzd-plugin.breeder"
    },
    "Exhibitions": {
      "type": "text"
    },
    "BreedSurvey": {
      "type": "text"
    }
  }
}
