{
  "kind": "singleType",
  "collectionName": "global_layouts",
  "info": {
    "singularName": "global-layout",
    "pluralName": "global-layouts",
    "displayName": "GlobalLayout"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Menu": {
      "type": "json"
    },
    "MenuStyle": {
      "type": "enumeration",
      "required": true,
      "default": "Classic",
      "enum": [
        "Classic",
        "StickyTransparent"
      ]
    },
    "DrawerMenu": {
      "type": "json"
    },
    "Logo": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images"
      ]
    },
    "Footer": {
      "type": "component",
      "component": "layout.footer",
      "repeatable": false
    },
    "Copyright": {
      "type": "string"
    },
    "SocialLinkFB": {
      "type": "string"
    },
    "SocialLinkYT": {
      "type": "string"
    },
    "PartnerLink": {
      "type": "component",
      "component": "links.partnerl-link",
      "repeatable": true
    },
    "Impressum": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::page.page"
    },
    "PrivacyPolicy": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    }
  }
}
