{
  "kind": "collectionType",
  "collectionName": "dogs",
  "info": {
    "singularName": "dog",
    "pluralName": "dogs",
    "displayName": "Dog"
  },
  "options": {
    "draftAndPublish": false
  },
  "pluginOptions": {
    "content-manager": {
      "visible": true
    }
  },
  "attributes": {
    "givenName": {
      "type": "string",
      "maxLength": 100
    },
    "fullKennelName": {
      "type": "string",
      "maxLength": 500
    },
    "avatar": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files"
      ]
    },
    "microchipNo": {
      "type": "string",
      "maxLength": 30
    },
    "dateOfBirth": {
      "type": "date"
    },
    "dateOfDeath": {
      "type": "date"
    },
    "sex": {
      "type": "enumeration",
      "enum": [
        "M",
        "F"
      ]
    },
    "color": {
      "type": "enumeration",
      "enum": [
        "S",
        "SM",
        "B"
      ]
    },
    "withersHeight": {
      "type": "integer"
    },
    "father": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::hzd-plugin.dog",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "sex"
            },
            "M"
          ]
        }
      }
    },
    "mother": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::hzd-plugin.dog",
      "conditions": {
        "visible": {
          "==": [
            {
              "var": "sex"
            },
            "F"
          ]
        }
      }
    },
    "cId": {
      "type": "integer",
      "unique": true
    },
    "cBreederId": {
      "type": "integer"
    },
    "cOwnerId": {
      "type": "integer"
    },
    "MemosDraft": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    },
    "MemosReleased": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    },
    "owner": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "breeder": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::hzd-plugin.breeder"
    },
    "Exhibitions": {
      "type": "text"
    },
    "BreedSurvey": {
      "type": "text"
    },
    "cFertile": {
      "type": "boolean"
    },
    "cStudBookNumber": {
      "type": "string"
    },
    "cStudBookNumberFather": {
      "type": "string"
    },
    "cStudBookNumberMother": {
      "type": "string"
    },
    "Images": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images"
      ]
    },
    "DogDocument": {
      "type": "component",
      "component": "breeding.dog-document",
      "repeatable": true
    },
    "Disabled": {
      "type": "boolean"
    }
  }
}
