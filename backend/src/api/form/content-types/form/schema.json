{
  "kind": "collectionType",
  "collectionName": "forms",
  "info": {
    "singularName": "form",
    "pluralName": "forms",
    "displayName": "Form"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "Name": {
      "type": "string"
    },
    "FormFields": {
      "type": "dynamiczone",
      "components": [
        "form.fld-group-separator",
        "form.fld-static-text",
        "form.short-text-input",
        "form.choice",
        "form.boolean-choice",
        "form.email-adress",
        "form.number-input",
        "form.form-submit-button",
        "form.text-area",
        "form.standard-identifiers"
      ]
    },
    "InclPrivacyPolicy": {
      "type": "boolean",
      "required": true,
      "default": true
    },
    "ThankYouMessage": {
      "type": "blocks"
    },
    "EventAdmin": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "plugin::users-permissions.user",
      "mappedBy": "form"
    }
  }
}
