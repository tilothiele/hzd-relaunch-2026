{
  "kind": "collectionType",
  "collectionName": "contacts",
  "info": {
    "singularName": "contact",
    "pluralName": "contacts",
    "displayName": "Contact"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "avatar": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "position": {
      "type": "integer"
    },
    "member": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "plugin::users-permissions.user"
    },
    "contact_group": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::contact-group.contact-group",
      "inversedBy": "contacts"
    },
    "Headline": {
      "type": "string"
    },
    "Name": {
      "type": "string"
    },
    "Street": {
      "type": "string"
    },
    "ZipCity": {
      "type": "string"
    },
    "Phone": {
      "type": "string"
    },
    "Email1": {
      "type": "email"
    },
    "Email2": {
      "type": "email"
    },
    "Introduction": {
      "type": "customField",
      "customField": "plugin::ckeditor5.CKEditor",
      "options": {
        "preset": "defaultHtml"
      }
    }
  }
}
